        },
        {
            "textBox": { "name": "", "text": "...", "speed": 40 },
            "background": { "bg": "Darkness"}
        },
        {
            "textBox": { "name": "", "text": "Do not question.", "speed": 40 },
            "background": { "bg": "Darkness"}
        },
        {
            "textBox": { "name": "", "text": "Second rule of the covenant.", "speed": 40 },
            "background": { "bg": "Darkness"}
        },
        {
            "textBox": { "name": "", "text": "Information must be gathered, threats need to be removed.. Orders are not to be questioned.", "speed": 40 },
            "background": { "bg": "Darkness"}
        },
        {
            "textBox": { "name": "", "text": "Bonding is meaningless. Another mission, another temporary ally. They don't know me, I don't know them.", "speed": 40 },
