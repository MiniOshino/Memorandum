            "background": { "bg": "Darkness"}
        },
        {
            "textBox": { "name": "", "text": "Parents, Bosses, Allies.. Yourself.", "speed": 40 },
            "background": { "bg": "Darkness"}
        },
        {
            "textBox": { "name": "", "text": "If someone dances out of line they are replaced. No redemptions, no second chances.", "speed": 40 },
            "background": { "bg": "Darkness"}
        },
        {
            "textBox": { "name": "", "text": "As far as I'm concerned, someone leaving me, might as well have been their death.", "speed": 40 },
